{
	"name": "hojalatheef/sitepackage",
	"description": "This is a base sitepackage extends t3kit themes",
	"type": "typo3-cms-extension",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Hoja Mustaffa Abdul Latheef",
			"email": "hlatheef@jweiland.net"
		}
	],
	"autoload": {
		"psr-4": {
			"HojaLatheef\\Sitepackage\\": "Classes/"
		}
	},
	"replace": {
		"sitepackage": "self.version",
		"hojalatheef/sitepackage": "self.version"
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": ".Build/Web"
		}
	}
}
